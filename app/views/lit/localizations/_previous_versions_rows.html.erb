<table class="table">
  <thead>
      <tr>
        <th><%= t('lit.common.previous_versions') %></th>
        <th width="25%"><%= t('lit.common.created_at') %></th>
        <th><span class="pull-right"><%= draw_icon 'remove', :class=>"close_versions" %></span></th>
      </tr>
  </thead>
  <tbody>
      <% @versions.each do |v| %>
        <tr>
          <td>
            <%= render :partial=>"/lit/localization_keys/localization_row", :locals=>{:localization=>v.translated_value} %>
          </td>
          <td colspan="2"><%= v.created_at %></td>
        </tr>
      <% end %>
  </tbody>
</table>
